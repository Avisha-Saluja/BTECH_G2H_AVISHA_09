#include<stdio.h>
int main()
{
    int a,b,c,d,e;
    int sum;
    float percentage;
    printf("enter marks of 5 subjects\n");
    scanf("%d%d%d%d%d",&a,&b,&c,&d,&e);
    sum=a+b+c+d+e;
    percentage=sum*100/500;
    printf("sum is %d \n",sum);
    printf("percentage is %f",percentage);
    return 0;
    }
