#include<stdio.h>
int main()
{
    char ch;
    printf("enter a character");
    scanf("%c",&ch);
    if(ch>='0'&&ch<='9')
    printf("It is a digit");
   else if(ch>='a'&& ch<= 'b')
     printf("It is a character");
     else  if(ch>='A'&& ch<= 'B')
      printf("It is a character");
      else
      printf("It is a special character");
}
